{
  "format_version": "1.18.20",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:player",
      "is_spawnable": false,
      "is_summonable": false,
      "is_experimental": false,
      "animations": {
        "on_load": "controller.animation.player.load"
      },
      "scripts": {
        "animate": [
          "on_load"
        ]
      }
    },

    "component_groups": {
      "minecraft:add_bad_omen": {
        "minecraft:spell_effects": {
          "add_effects": [
            {
              "effect": "bad_omen",
              "duration": 6000,
              "visible": false,
              "display_on_screen_animation": true
            }
          ]
        },
        "minecraft:timer": {
          "time": [ 0.0, 0.0 ],
          "looping": false,
          "time_down_event": {
            "event": "minecraft:clear_add_bad_omen",
            "target": "self"
          }
        }
      },
      "minecraft:clear_bad_omen_spell_effect": {
        "minecraft:spell_effects": {
        }
      },
      "minecraft:raid_trigger": {
        "minecraft:raid_trigger": {
          "triggered_event": {
            "event": "minecraft:remove_raid_trigger",
            "target": "self"
          }
        },
        "minecraft:spell_effects": {
          "remove_effects": "bad_omen"
        }
      },


      //========================================
      // Addon
      //========================================
      "max:leashed_bee_sensor_for_enabling_flying": {
        "minecraft:entity_sensor": {
          "sensor_range": 10,
          "relative_range":false,
          "minimum_count": 2,
          "maximum_count": 2,
          "event_filters": {
            "all_of": [
              {"test": "is_family", "subject": "other", "value": "bee"},
              {"test": "is_leashed", "subject": "other", "value": true}
            ]
          },
          "event": "max:become_bee_flight_capable"
        }
      },
      "max:fam_and_var_when_not_bee_flight_capable": {
        "minecraft:type_family": {
          "family": [ "player"]
        },
        "minecraft:variant": {
          "value": 0
        }
      },

      "max:leashed_bee_sensor_for_disabling_flying": {
        "minecraft:entity_sensor": {
          "sensor_range": 10,
          "relative_range":false,
          "minimum_count": 0,
          "maximum_count": 1,
          "event_filters": {
            "all_of": [
              {"test": "is_family", "subject": "other", "value": "bee"},
              {"test": "is_leashed", "subject": "other", "value": true}
            ]
          },
          "event": "max:become_bee_flight_incapable"
        }
      },
      "max:fam_and_var_when_bee_flight_capable": {
        "minecraft:type_family": {
          "family": [ "player", "bee_flying" ]
        },
        "minecraft:variant": {
          "value": 1    //For 'riding' bee_swarm. 0 will probs be for not riding anything
        }
      },

      "max:bee_target_for_no_movement": {     //stationary
        "minecraft:rideable": {
          "seat_count": 1,
          "pull_in_entities": true,
          "family_types": [
            "bee_flying_target"
          ],
          "seats": [
            {
              "position": [ 0.0, 4.0, 0.0 ],
                "min_rider_count": 0,
                "max_rider_count": 0,
                "lock_rider_rotation": 0
            }
          ]
        }
      },
      "max:bee_target_for_left_movement": {   //left
        "minecraft:rideable": {
          "seat_count": 1,
          "pull_in_entities": true,
          "family_types": [
            "bee_flying_target"
          ],
          "seats": [
            {
              "position": [ 4.0, 4.0, 0.0 ],
                "min_rider_count": 0,
                "max_rider_count": 0,
                "lock_rider_rotation": 0
            }
          ]
        }
      },
      "max:bee_target_for_right_movement": {  //right
        "minecraft:rideable": {
          "seat_count": 1,
          "pull_in_entities": true,
          "family_types": [
            "bee_flying_target"
          ],
          "seats": [
            {
              "position": [ -4.0, 4.0, 0.0 ],
              "min_rider_count": 0,
              "max_rider_count": 0,
              "lock_rider_rotation": 0
            }
          ]
        }
      },
      "max:bee_target_for_forward_movement": {  //forward
        "minecraft:rideable": {
          "seat_count": 1,
          "pull_in_entities": true,
          "family_types": [
            "bee_flying_target"
          ],
          "seats": [
            {
              "position": [ 0.0, 4.0, 4.0 ],
              "min_rider_count": 0,
              "max_rider_count": 0,
              "lock_rider_rotation": 0
            }
          ]
        }
      },
      "max:bee_target_for_back_movement": {   //back
        "minecraft:rideable": {
          "seat_count": 1,
          "pull_in_entities": true,
          "family_types": [
            "bee_flying_target"
          ],
          "seats": [
            {
              "position": [ 0.0, 4.0, -4.0 ],
              "min_rider_count": 0,
              "max_rider_count": 0,
              "lock_rider_rotation": 0
            }
          ]
        }
      },

      "max:default_family": {
        "minecraft:type_family": {
          "family": [ "player"]
        }
      }
      //========================================
      //========================================
    },



    "components": {
      "minecraft:variant": {
        "value": 0
      },
      "minecraft:interact": {
          "interactions": [
              {
                  "on_interact": {
                      "filters": {
                          "all_of": [
                              { "test": "has_equipment", "subject": "self", "domain": "head", "value": "max:flower_circlet"},
                              { "test" :  "is_family", "subject" : "other", "value" :  "player"},
                              { "test": "has_equipment", "subject": "other", "domain": "hand", "value": "shears"}
                          ]
                      },
                      "event": "max:cut_flower_circlet"
                  },
                  "play_sounds": "shear",
                  "swing": true
              }
          ]
      },
      "minecraft:experience_reward": {
        "on_death": "Math.Min(query.player_level * 7, 100)"
      },
      "minecraft:type_family": {
        "family": [ "player"]
      },
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.8
      },
      "minecraft:can_climb": {
      },
      "minecraft:movement": {
        "value": 0.1
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:attack": {
        "damage": 1
      },
      "minecraft:exhaustion_values": {
        "heal": 6,
        "jump": 0.05,
        "sprint_jump": 0.2,
        "mine": 0.005,
        "attack": 0.1,
        "damage": 0.1,
        "walk": 0.0,
        "sprint": 0.01,
        "swim": 0.01
      },
      "minecraft:player.saturation": {
        "value": 5,
         "max": 20
      },
      "minecraft:player.exhaustion": {
        "value": 0,
        "max": 20
      },
      "minecraft:player.level": {
        "value": 0,
        "max": 24791
      },
      "minecraft:player.experience": {
        "value": 0,
        "max": 1
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": -1,
        "inhale_time": 3.75,
        "generates_bubbles": false
      },
      "minecraft:nameable": {
        "always_show": true,
        "allow_name_tag_renaming": false
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:insomnia": {
        "days_until_insomnia": 3
      },
      "minecraft:rideable": {
        "seat_count": 2,
        "family_types": [
          "parrot_tame", "bee_flying_target"
        ],
        "pull_in_entities": true,
        "seats": [
          {
            "position": [ 0.4, -0.2, -0.1 ],
            "min_rider_count": 0,
            "max_rider_count": 0,
            "lock_rider_rotation": 0
          },
          {
            "position": [ -0.4, -0.2, -0.1 ],
            "min_rider_count": 1,
            "max_rider_count": 2,
            "lock_rider_rotation": 0
          }
        ]
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:block_climber": {},
      "minecraft:environment_sensor": {
        "triggers": {
          "filters": {
            "all_of": [
              {
                "test": "has_mob_effect",
                "subject": "self",
                "value": "bad_omen"
              },
              {
                "test": "is_in_village",
                "subject": "self",
                "value": true
              }
            ]
          },
          "event": "minecraft:trigger_raid"
        }
      }
    },

    "events": {
      //===================================
      // Bee Addon
      //===================================
      "max:cut_flower_circlet": {
        "run_command": {
          "command": [
            "replaceitem entity @s slot.armor.head 0 air",
            "give @s max:flower_string",
            "give @s max:rarer_flower_string"
          ],
          "target": "self"
        }
      },

      /*"max:bee_leashed": {
        "add": {
          "component_groups": [
            "max:leashed_bee_sensor_for_enabling_flying",
            "max:fam_and_var_when_not_bee_flight_capable"
          ]
        }
      },*/

      "max:become_bee_flight_capable":{
        "remove": {
          "component_groups": [
            "max:fam_and_var_when_not_bee_flight_capable",
            "max:leashed_bee_sensor_for_enabling_flying"
          ]
        },
        "add": {
          "component_groups": [
            "max:fam_and_var_when_bee_flight_capable",
            "max:leashed_bee_sensor_for_disabling_flying",
            "max:bee_target_for_no_movement"
          ]
        }
      },

      "max:become_bee_flight_incapable":{
        "remove": {
          "component_groups": [
            "max:fam_and_var_when_bee_flight_capable",
            "max:leashed_bee_sensor_for_disabling_flying",
            "max:bee_target_for_no_movement",
            "max:bee_target_for_left_movement",
            "max:bee_target_for_right_movement",
            "max:bee_target_for_forward_movement",
            "max:bee_target_for_back_movement"
          ]
        },
        "add": {
          "component_groups": [
            "max:fam_and_var_when_not_bee_flight_capable",
            "max:leashed_bee_sensor_for_enabling_flying"
          ]
        }
      },

      "max:bee_flying_stationary": {
        "remove": {
          "component_groups": [
            "max:bee_target_for_left_movement",
            "max:bee_target_for_right_movement",
            "max:bee_target_for_forward_movement",
            "max:bee_target_for_back_movement"
          ]
        },
        "add": {
          "component_groups": [
            "max:bee_target_for_no_movement"
          ]
        }
      },
      "max:bee_flying_left": {
        "remove": {
          "component_groups": [
            "max:bee_target_for_no_movement",
            "max:bee_target_for_right_movement",
            "max:bee_target_for_forward_movement",
            "max:bee_target_for_back_movement"
          ]
        },
        "add": {
          "component_groups": [
            "max:bee_target_for_left_movement"
          ]
        }
      },
      "max:bee_flying_right": {
        "remove": {
          "component_groups": [
            "max:bee_target_for_no_movement",
            "max:bee_target_for_left_movement",
            "max:bee_target_for_forward_movement",
            "max:bee_target_for_back_movement"
          ]
        },
        "add": {
          "component_groups": [
            "max:bee_target_for_right_movement"
          ]
        }
      },
      "max:bee_flying_forward": {
        "remove": {
          "component_groups": [
            "max:bee_target_for_no_movement",
            "max:bee_target_for_left_movement",
            "max:bee_target_for_right_movement",
            "max:bee_target_for_back_movement"
          ]
        },
        "add": {
          "component_groups": [
            "max:bee_target_for_forward_movement"
          ]
        }
      },
      "max:bee_flying_back": {
        "remove": {
          "component_groups": [
            "max:bee_target_for_no_movement",
            "max:bee_target_for_left_movement",
            "max:bee_target_for_right_movement",
            "max:bee_target_for_forward_movement"
          ]
        },
        "add": {
          "component_groups": [
            "max:bee_target_for_back_movement"
          ]
        }
      },
      /*"max:remove_all_bee_flying_sensors": {
        "remove": {
          "component_groups": [
            "max:leashed_bee_sensor_to_enable_flying",
            "max:leashed_bee_sensor_to_disable_flying",
            "max:bee_target_for_no_movement",
            "max:bee_target_for_left_movement",
            "max:bee_target_for_right_movement",
            "max:bee_target_for_forward_movement",
            "max:bee_target_for_back_movement"
          ]
        },
        "add": {
          "component_groups": [
            "max:default_family"
          ]
        }
      },*/
      //===================================
      //===================================

      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "max:leashed_bee_sensor_for_enabling_flying",
            "max:fam_and_var_when_not_bee_flight_capable"
          ]
        }
      },

      "minecraft:gain_bad_omen": {
        "add": {
          "component_groups": [
            "minecraft:add_bad_omen"
          ]
        }
      },
      "minecraft:clear_add_bad_omen": {
        "remove": {
          "component_groups": [
            "minecraft:add_bad_omen"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:clear_bad_omen_spell_effect"
          ]
        }
      },
      "minecraft:trigger_raid": {
        "add": {
          "component_groups": [ "minecraft:raid_trigger" ]
        }
      },
      "minecraft:remove_raid_trigger": {
        "remove": {
          "component_groups": [ "minecraft:raid_trigger" ]
        }
      }
    }
  }
}